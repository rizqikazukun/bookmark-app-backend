generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int      @id @default(autoincrement())
  first_name String?
  last_name  String?
  email      String
  password   String
  createdAt  DateTime @default(now())
  updateAt   DateTime @updatedAt
}

model Bookmark {
  id          Int      @id @default(autoincrement())
  title       String
  description String?
  link        String
  createdAt   DateTime @default(now())
  updateAt    DateTime @updatedAt
}
